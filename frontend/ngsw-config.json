{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": ["/favicon.ico", "/index.html", "/manifest.webmanifest", "/*.css", "/*.js"],
        "urls": ["https://fonts.googleapis.com/icon?family=Material+Icons", "https://fonts.googleapis.com/css?family=Roboto:300,400,500&amp;display=swap"]
      }
    },
    {
      "name": "login",
      "installMode": "lazy",
      "resources": {
        "files": ["app/login/login.component.html", "/*.css", "/*.js"]
      }
    },
    {
      "name": "collectionpanel",
      "installMode": "lazy",
      "resources": {
        "files": ["app/library/collections/collection-panel/collection-panel.component.html", "/*.css", "/*.js"]
      }
    },
    {
      "name": "collectiontable",
      "installMode": "lazy",
      "resources": {
        "files": ["app/library/collections/collection-views/collection-datatable/collection-datatable.component.html", "/*.css", "/*.js"]
      }
    },
    {
      "name": "library",
      "installMode": "lazy",
      "resources": {
        "files": ["app/library/offline-library.component.html", "/*.css", "/*.js"]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": ["/assets/**", "/*.(eot|svg|cur|jpg|png|webp|gif|otf|ttf|woff|woff2|ani)"]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "favorites",
      "urls": ["api/collections/favorites"],
      "cacheConfig": {
        "maxSize": 1,
        "maxAge": "24h",
        "strategy": "performance"
      }
    }
  ]
}
